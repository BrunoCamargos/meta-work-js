{
  "name": "meta-work-js",
  "todo": "criar docker image coveralls ou travis",
  "version": "1.4.0",
  "description": "Um template para outros projetos NodeJS com ES6",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && babel lib -d dist/lib && cp package.json dist",
    "mocha": "_mocha",
    "lint": "./node_modules/.bin/eslint ./**.js .",
    "pretest": "npm run lint && ./node_modules/.bin/depcheck --ignores express,eslint-config-airbnb,eslint-plugin-import,eslint-plugin-jsx-a11y,eslint-plugin-react && ./node_modules/.bin/snyk test",
    "test": "./node_modules/.bin/istanbul cover _mocha",
    "posttest": "./node_modules/.bin/istanbul check-coverage",
    "debug": "export NODE_ENV=dev && _mocha --debug-brk",
    "start": "npm run build && node dist/lib/index.js",
    "serve": "./node_modules/.bin/nodemon --exec 'npm run start'",
    "preversion": "npm test",
    "version": "rm -rf dist/ coverage/",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrunoCamargos/meta-work-js.git"
  },
  "author": "Bruno Camargos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BrunoCamargos/meta-work-js/issues"
  },
  "homepage": "https://github.com/BrunoCamargos/meta-work-js#readme",
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.16.0",
    "babel-register": "^6.16.3",
    "chai": "^3.5.0",
    "del": "^2.2.2",
    "depcheck": "^0.6.4",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-istanbul": "^1.1.1",
    "gulp-load-plugins": "^1.3.0",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "isparta": "^4.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.1.0",
    "nodemon": "^1.11.0",
    "snyk": "^1.19.1"
  },
  "dependencies": {
    "express": "^4.14.0"
  }
}
